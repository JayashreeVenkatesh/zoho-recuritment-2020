package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
	// write your code here
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int temp = N;
        for(int i = 0; i< N; i++){
            for(int j = 0 ; j < temp; j++){
                if( i <= j){
                    System.out.print(N-i);
                }else{
                    System.out.print(N-j);
                }
            }
            System.out.println();
        }
    }
}
